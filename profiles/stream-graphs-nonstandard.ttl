@prefix :        <https://w3id.org/riverbench/profiles/> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rb:      <https://w3id.org/riverbench/schema/metadata#> .

:stream-graphs-nonstandard
    a dcat:DatasetSeries, rb:Profile ;
    dcterms:identifier "stream-graphs-nonstandard" ;
    dcterms:title "Streaming graphs (non-standard)" ;
    dcterms:description "Streaming graphs (with non-standard extensions)" ;
    rb:isSupersetOfProfile :stream-graphs ;
    rb:hasRestriction [
        rb:hasStreamElementType rb:graphs ;
    ] , [
        rb:hasDistributionType rb:graphStreamDistribution ;
    ] , [
        rb:usesRdfStar false ;
    ]
.
