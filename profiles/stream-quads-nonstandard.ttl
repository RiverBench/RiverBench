@prefix :        <https://w3id.org/riverbench/profiles/> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rb:      <https://w3id.org/riverbench/schema/metadata#> .

:stream-quads-nonstandard
    a dcat:DatasetSeries, rb:Profile ;
    dcterms:identifier "stream-quads-nonstandard" ;
    dcterms:title "Streaming quads (non-standard)" ;
    dcterms:description "Streaming quads (with non-standard extensions)" ;
    rb:isSupersetOfProfile :stream-quads, :stream-graphs-nonstandard ;
    rb:hasRestriction [
        rb:hasStreamElementType rb:quads, rb:graphs ;
    ] , [
        rb:hasDistributionType rb:quadStreamDistribution, rb:graphStreamDistribution ;
    ] , [
        rb:usesRdfStar false ;
    ]
.
