@prefix :        <https://w3id.org/riverbench/profiles/> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rb:      <https://w3id.org/riverbench/schema/metadata#> .

:stream-quads
    a dcat:DatasetSeries, rb:Profile ;
    dcterms:identifier "stream-quads" ;
    dcterms:title "Streaming quads (standard)" ;
    dcterms:description "Streaming quads (RDF 1.1 standard only)" ;
    rb:isSupersetOfProfile :stream-graphs ;
    rb:hasRestriction [
        rb:hasStreamElementType rb:quads, rb:graphs ;
    ] , [
        rb:hasDistributionType rb:quadStreamDistribution, rb:graphStreamDistribution ;
    ] , [
        rb:conformsToRdf11 true ;
    ]
.
