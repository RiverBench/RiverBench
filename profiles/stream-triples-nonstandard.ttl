@prefix :        <https://riverbench.github.io/profiles/> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rb:      <https://riverbench.github.io/schema/dataset#> .

:stream-triples-nonstandard
    a dcat:DatasetSeries, rb:Profile ;
    dcterms:identifier "stream-triples-nonstandard" ;
    dcterms:title "Streaming triples (non-standard)" ;
    dcterms:description "Streaming triples (with non-standard extensions)" ;
    rb:isSupersetOfProfile :stream-triples ;
    rb:hasRestriction [
        rb:hasStreamElementType rb:triples ;
    ] , [
        rb:hasDistributionType rb:tripleStreamDistribution ;
    ] , [
        rb:usesRdfStar false ;
    ]
.
